<button class="keep" bindtap="bindToAdd">添加</button>
<view class="swiper-tab">
  <view class="swiper-tab-list {{currentTab==0 ? 'on' : ''}}" data-current="0" bindtap="swichNav">全部</view>
  <view class="swiper-tab-list {{currentTab==1 ? 'on' : ''}}" data-current="1" bindtap="swichNav">已支付</view>
  <view class="swiper-tab-list {{currentTab==2 ? 'on' : ''}}" data-current="2" bindtap="swichNav">未支付</view>
</view>

<swiper current="{{currentTab}}" class="swiper-box" duration="300" style="height:{{winHeight - 31}}px"
  bindchange="bindChange">
  <!-- 全部 -->
  <swiper-item>
    <view id="wrapper">
      <ul>
        <block wx:for-items="{{datass}}" wx:key="id">
          <li class="registration my_active">
             <view class="registration_time">收件人姓名:<text>{{item.receiver	}}</text></view>
 <view class="registration_time">收件地址:<text>{{item.address	}}</text></view>
 <view class="registration_time">收件人手机号:<text>{{item.receivetel	}}</text></view>
 <view class="registration_time">快递单号:<text>{{item.expressno	}}</text></view>
 <view class="registration_time">楼层:<text>{{item.floorr	}}</text></view>
 <view class="registration_time">快递员id:<text>{{item.courierid	}}</text></view>
 <view class="registration_time">快递员姓名:<text>{{item.couriername	}}</text></view>
 <view class="registration_time">快递员性别:<text>{{item.couriergender	}}</text></view>
 <view class="registration_time">悬赏金额:<text>{{item.price	}}</text></view>
 <view class="registration_time">取件人ID:<text>{{item.collectid	}}</text></view>
 <view class="registration_time">取件人姓名:<text>{{item.collectname	}}</text></view>
 <view class="registration_time">取件人手机号:<text>{{item.collecttel	}}</text></view>
 <view class="registration_time">取件人楼层:<text>{{item.collectfloor	}}</text></view>
 <view class="registration_time">取件人房间号:<text>{{item.collectdoorno	}}</text></view>
 <view class="registration_time">取件费用:<text>{{item.collectprice	}}</text></view>
 <view class="registration_time">取件时间:<text>{{item.collecttime	}}</text></view>
 <view class="registration_time">用户ID:<text>{{item.userid	}}</text></view>

            <view>
              <text class="active_num wait" bindtap="bindToDetail" id="{{item.id}}">详情</text>
              <text class="active_num order" bindtap="bindToEdit" id="{{item.id}}">修改</text>
              <text class="active_num history" bindtap="bindToAdd" id="{{item.id}}">新增</text>
              <text class="active_num" bindtap="bindToDel" id="{{item.id}}">删除</text>
            </view>
          </li>
        </block>
      </ul>
    </view>
  </swiper-item>


